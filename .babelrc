{
  "presets": [
    ["env", { "modules": false }],
    "es2015", "es2016", "es2017", "react"
  ],
  "plugins": ["transform-runtime", "react-hot-loader/babel"],
  "comments": false,
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
